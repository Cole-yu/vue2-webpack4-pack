{
  "name": "vue2-webpack4-pack",
  "version": "1.0.4",
  "homepage": "https://github.com/Cole-yu/vue2-webpack4-pack#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cole-yu/vue2-webpack4-pack.git"
  },
  "description": "开箱即用的 vue2 + Webpack4 项目模板",
  "main": "src/main.js",
  "private": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.js",
    "clean": "rimraf dist",
    "package": "cross-env NODE_ENV=production webpack --config build/webpack.prod.js",
    "build": "npm run clean && npm run package"
  },
  "keywords": [
    "Webpack4",
    "Vue2",
    "npm"
  ],
  "author": "cole.yu",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "babel-loader": "^8.3.0",
    "copy-webpack-plugin": "^6.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.2",
    "less": "^4.2.0",
    "less-loader": "^7.3.0",
    "mini-css-extract-plugin": "^1.6.2",
    "postcss": "^8.4.31",
    "postcss-loader": "^4.3.0",
    "postcss-preset-env": "^7.8.3",
    "progress-bar-webpack-plugin": "^2.1.0",
    "rimraf": "^3.0.2",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.11.1",
    "vue-template-compiler": "^2.7.15",
    "webpack": "^4.47.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "core-js": "^3.33.3",
    "vue-router": "3.6.5"
  },
  "engines": {
    "node": ">=12.18.4",
    "npm": ">=6.14.6"
  },
  "sideEffects": [
    "dist/*",
    "*.css",
    "*.less",
    "*.vue"
  ],
  "config": {
    "host": "0.0.0.0",
    "port": 8080
  }
}
